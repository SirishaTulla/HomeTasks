function getMiddle(s)
{
  if (s.length < 3) {
    return s
  }
  if (s.length > 2) {
    if (s.length % 2 === 0) {
      let middleIndex1 = (s.length/2)-1
      let middleIndex2 = s.length/2
      return s[middleIndex1] + s[middleIndex2]
    } else {
      let middleIndex = Math.floor(s.length/2)
      return s[middleIndex]
    }
  }
}
